{
    "name": "mrcrmn/backbone",
    "version": "0.1.0",
    "description": "A simple Framework with basic routing, controllers and blade.",
    "authors": [
        {
            "name": "Marco Reimann",
            "email": "marcoreimann@outlook.de"
        }
    ],
    "require": {
        "symfony/http-kernel": "^4.0",
        "symfony/http-foundation": "^4.0",
        "symfony/dotenv": "^4.0",
        "nikic/fast-route": "^1.3",
        "duncan3dc/blade": "^4.2",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0"
    },
    "require-dev": {
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4" : {
            "Backbone\\": "src/",
            "App\\": "app/"
        },
        "files": [
            "src/resources/functions/helpers.php"
        ]
    }
}
